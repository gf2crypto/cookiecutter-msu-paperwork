 @article{Sidelnikov3,
    title={О системе шифрования, построенной на основе обобщенных кодов Рида--Соломона},
    volume={4},
    abstractNote={В работах [1, 2] на базе теоретико-кодовых конструкций предложены методы построения системы открытого шифрования. Их основой является общеизвестная матрица B размера (s+1)×N с элементами из конечного поля Fq вида B=H⋅A, где A – некоторая неизвестная матрица, являющаяся проверочной матрицей q-значного обобщенного кода Рида – Соломона кода (ОРС-код), в частности, кода Гоппы, а H – неизвестная невырожденная матрица размеров (s+1)×(s+1). 
    В настоящей работе предложен метод нахождения неизвестных матриц A,H с элементами из поля Fq, который определяет матрицу B, за O(s4+sN) операций. Тем самым устанавливается ненадежность рассматриваемых систем открытого шифрования.},
    number={3},
    journal={Дискретная математика},
    author={Сидельников, В.М. and Шестаков, С.О.},
    year={1992},
    pages={57–63}
}

